<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.khwy.mapper.RoleMapper">
	<select id="findById" parameterType="String" resultType="com.khwy.pojo.Role">
		select 
			roleid,rolename,perids 
		from
			t_role
		<where>
			1=1
			and roleid = #{roleid}
		</where>
	</select>
</mapper>